      <figure>
        <img class ="image_selection2 hue"  src="" id ="hue" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 blur" src="" id ="blur" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 contrast" src="" id ="contrast" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 sepia" src="" id ="sepia" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 invert" src="" id ="invert" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 greyscale" src="" id ="greyscale" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 saturate" src="" id ="saturate" width="150" height="150" alt="">
      </figure>
      <figure>
        <img class ="image_selection2 opacity" src="" id ="opacity" width="150" height="150" alt="">
      </figure>

  </div>


  <div id ="artistInformation">
  </div>
  <div id ="artistInformation1">
  </div>


</div>
